{
  "manifest_version": 3,
  "name": "tcvet Extension",
  "version": "0.9.5",
  "description": "Thomas's ezyvet Extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon48.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "sidePanel"
  ],
  "host_permissions": [
    "https://utcvm.use1.ezyvet.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://utcvm.use1.ezyvet.com/*"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "options_page": "options/options.html"
}